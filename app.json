{
  "expo": {
    "name": "expo-sdk53-react-native-maps",
    "slug": "expo-sdk53-react-native-maps",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "exposdk53reactnativemaps",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "assetBundlePatterns": ["**/*"],
    "splash": {
      "foregroundImage": "./assets/splash.png",
      "backgroundColor": "#666666"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.exposdk53reactnativemaps",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.yourcompany.exposdk53reactnativemaps"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "https://github.com/expo/expo/blob/master/templates/expo-template-blank/assets/splash.png?raw=true",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d4ec95b6-4e63-4f1d-9b50-0ed9bcfa365d"
      }
    },
    "owner": "sugasaki"
  }
}
